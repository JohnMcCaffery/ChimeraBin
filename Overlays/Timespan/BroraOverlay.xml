		<Features>

			<Feature Factory="Image" Name="RaiseArmPrompt" X="400" Y="20" Frame="LeftWindow">Images/Misc/RaiseArmPrompt.png</Feature>

			<Feature Factory="Image" Name="RaiseArmPromptExplore" X="400" Y="20" Frame="LeftWindow">Images/Misc/RaiseArmPrompt.png</Feature>

			<Feature Factory="StaticText" Name="SurveyText" X="40" Y="40" Font="Verdana" Colour="White" Size="60" Frame="LeftWindow">These images show the process 
				of investigation which informed
				the creation of Virtual Caen.
			</Feature>

			<Feature Factory="Image" Name="HelpSidebarAvatarL"  X="50" Y="150" Frame="LeftWindow" >Images/Misc/HelpSidebarAvatar.png</Feature>
