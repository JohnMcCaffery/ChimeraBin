			</State>

			<!-- 			Guided Tour Flythrough  			-->
			<State Factory="Flythrough" Name="GuidedTour" File="Flythroughs/Timespan/Guided.xml" AutoStep="true" Loop="false" DisplaySubtitles="true" SubtitleTimeout="11">

				<SubtitleText Frame="MainWindow" Font="Verdana" Size="30" Colour="Yellow" Style="Regular" X="960" Y="978" Alignment="TopCenter" />

				<IdleTriggers>
					<Trigger Factory="Simple" />
				</IdleTriggers>


				<Steps>

					<Step Step="0" Voiceover="Audio/1.mp3" >
						<Features>
							<Feature Name="RaiseArmPrompt" />
						</Features>
					</Step>
					<Step Step="1" Voiceover="Audio/2.mp3" />
					<Step Step="2" Voiceover="Audio/3.mp3" />
					<Step Step="3" Voiceover="Audio/4.mp3" />
					<Step Step="4" Voiceover="Audio/5.mp3" />
